        "copyjs": "npm run ts2js & webpack --config  ./webpack.config.copyjs.js",
        "doc": "npx typedoc ./src/source/index.ts"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
